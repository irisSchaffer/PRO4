{% extends 'PRO4ProjectBundle:UsersInProject:overview.html.twig' %}

{% block form %}
	<form method="post">
		<fieldset>
			<legend>Add User to Project</legend>
			<ul>
				<li>{{ form_row(form.email) }}</li>
			</ul>
		</fieldset>
		{{ form_widget(form) }}

		<button type="submit">Add</button>
	</form>	
{% endblock %}